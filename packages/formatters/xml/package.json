{
  "name": "@typed-prompt/xml-formatter",
  "version": "0.1.1",
  "description": "A XML formatter for the @typed-prompt ecosystem. Enables strongly-typed prompt values to be formatted as XML strings for flexible presentation and integration with AI workflows.",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "prepublishOnly": "bun run clear-package-json package.json -o package.json",
    "postpublish": "git restore .",
    "build": "tsdown"
  },
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "development": "./src/index.ts",
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "files": ["dist"],
  "devDependencies": {
    "@types/bun": "latest",
    "@types/jstoxml": "^5.0.0",
    "@typed-prompt/types": "workspace:*"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "jstoxml": "^7.0.1"
  },
  "author": { "name": "Askar Yusupov" },
  "repository": {
    "type": "git",
    "url": "https://github.com/pyoner/typed-prompt"
  }
}
